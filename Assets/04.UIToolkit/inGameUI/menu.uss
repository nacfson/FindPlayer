.scoreLabel {
    -unity-font-definition: url('project://database/Assets/09.Fonts/210%20%EC%BD%A4%ED%93%A8%ED%83%80%EC%84%B8%ED%83%81R%20SDF%201.asset?fileID=11400000&guid=58a9052674fd8424aba65a760159f21f&type=2#210 콤퓨타세탁R SDF 1');
    -unity-font-style: bold;
    -unity-text-align: middle-left;
    font-size: 20px;
    flex-grow: 1;
    padding-left: 30px;
    padding-right: 30px;
    padding-top: 30px;
    padding-bottom: 30px;
}

.killLogUI {
    height: 109px;
    background-color: rgba(82, 82, 82, 0.54);
    border-top-left-radius: 48px;
    border-bottom-left-radius: 48px;
    border-top-right-radius: 48px;
    border-bottom-right-radius: 48px;
    left: 100%;
    transition-duration: 1s;
}

.killLogUI.active {
    left: 0;
    transition-duration: 1s;
}
